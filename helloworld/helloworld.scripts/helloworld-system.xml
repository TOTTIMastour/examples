<system xmlns="http://schema.paremus.com/sf/1" name="helloworld-system"
	boundary="fibre" repopath="helloworld-ext,helloworld,aries-ext,aries">

	<description>
		Deploys a Blueprint-based Greeting service and a CLI
		command.
	</description>
	<nature group="demo" />

	<system.part category="ms" name="com.example.hello">
		<property name="language" value="de" />
	</system.part>


	<system.part category="osgi.active.bundle" name="helloworld.blueprint" />
	<system.part category="osgi.active.bundle" name="helloworld.cli" />

</system>