Private-Package: com.paremus.packager.nginx.unix
Conditional-Package: aQute.lib*
Include-Resource: data=nginx/linux64

Require-Capability: \
	packager.manager; effective:=active, \
	osgi.native;filter:="(& \
		(osgi.native.osname=Linux) \
		(osgi.native.processor=x86-64) \
	)"
Provide-Capability: packager.type;package.type=nginx;version:Version=1.2.8;\
	effective:="active";uses:="org.bndtools.service.packager"

Service-Component: *